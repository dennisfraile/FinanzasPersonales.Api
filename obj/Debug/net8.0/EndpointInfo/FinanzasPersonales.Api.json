          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "nombre": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "tipo": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "userId": {
            "minLength": 1,
